{
  "name": "system_register",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "bnk",
  "sourceRoot": "apps/system_register/src",
  "tags": [],
  "targets": {
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "buildTarget": "system_register:build"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/system_register/jest.config.ts",
        "tsConfig": "apps/system_register/tsconfig.spec.json"
      }
    },
    "serve-static": {
      "continuous": true,
      "executor": "@nx/web:file-server",
      "defaultConfiguration": "production",
      "options": {
        "buildTarget": "system_register:build",
        "port": 4201,
        "watch": false
      },
      "configurations": {
        "development": {
          "buildTarget": "system_register:build:development"
        },
        "production": {
          "buildTarget": "system_register:build:production"
        }
      }
    },
    "serve": {
      "options": {
        "port": 4201
      },
      "dependsOn": ["dashboard:serve"]
    }
  }
}
